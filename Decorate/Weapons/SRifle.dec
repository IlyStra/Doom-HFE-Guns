	SNP0 A 1 Offset(4,38)
	SNP0 A 1 Offset(5,40)
	SNP0 A 1 Offset(6,42)
    SNP0 A 2 Offset(6,45)
	SNP0 A 2 Offset(7,50)
	SNP0 A 2 Offset(8,55)
	SNP0 A 2 Offset(9,60)
	SNP0 A 2 Offset(10,65)
