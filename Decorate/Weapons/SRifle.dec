	SNP0 A 3 Offset(12,67)
	SNP0 A 3 Offset(13,68)
	SNP0 A 3 Offset(13,68)
	SNP0 A 3 Offset(13,68)
	SNP0 A 3 Offset(13,68)
	SNP0 A 3 Offset(13,68)
